<div id="faq-category-show" class="container">
  <div class="row">
    <article class="col-12">
      <h3><%=@faq_category.title %></h3>
      <p>
        <b><%=t(:enable,scope:[:activerecord,:attributes,:faq_category]) %></b>
      </p>
      <p>
        <b><%= t(:created_at,scope:[:activerecord,:attributes,:faq_category]) %></b>
        <%= @faq_category.created_at %>
      </p>
      <p>
        <b><%= t(:updated_at,scope:[:activerecord,:attributes,:faq_category]) %></b>
        <%= @faq_category.updated_at %>
      </p>
    </article>
  </div>
  <div class="row">
    <div class="col-12 show-bottom-menu">
      <div class="row">
        <div class="col-12 col-lg-6">
          <%=link_to t(:link_list),admin_faq_categories_path,:class=>"btn btn-secondary" %>
        </div>
        <div class="text-right col-12 col-lg-6">
          <%=link_to t(:edit),edit_admin_faq_category_path(@faq_category),:class=>"btn btn-secondary" %>
          <%=link_to t(:delete),admin_faq_category_path(@faq_category),:class=>"btn btn-danger" %>
        </div>
      </div>
    </div>
  </div>
</div>
