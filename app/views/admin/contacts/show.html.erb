<div id="notices" class="container sub-main">
  <div class="row">
    <article class="slboard_content" itemscope itemprop="article" itemtype="http://schema.org/Article">
      <div class="sl_content_header">
        <h3 itemprop="name"><%= @contact.title %></h3>
        <p>작성자 :<%= @contact.name %>&nbsp;&nbsp;&nbsp; 작성일 : <%= @contact.created_at %></p>
      </div>
      <div class="sl_content_main">
        <div class="sl_content_text" itemprop="text"><%= simple_format(@contact.contact_content.content) %></div>
      </div>
    </article>
    <div id="sl_content_bottom_menu">
      <%= link_to t(:link_list),admin_contacts_path %>
          &nbsp; | &nbsp; <%= link_to t(:link_edit), edit_admin_contact_path(@contact) %> &nbsp; | &nbsp; <%= link_to t(:link_delete), [:admin,@contact], method: :delete, data: { confirm: t(:delete_confirm) } %>

    </div>
  </div>
</div>
