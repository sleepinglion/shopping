<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
      	<Cell><Data ss:Type="String"><%=t(:label_period) %></Data></Cell>
      		<% unless(params[:member_type]=='2') %>
      	<Cell><Data ss:Type="String"><%=t(:label_auth_member) %></Data></Cell>
      		<% end %>
      		<% unless(params[:member_type]=='1') %>
      	<Cell><Data ss:Type="String"><%=t(:label_no_auth_member) %></Data></Cell>
					<% end %>
			</Row>
      <Row>
      	<% unless @stats.empty? %>
      	<% @stats.each do |stat| %>
      	<Cell><Data ss:Type="String"><%=stat.period %><%=period_format(params[:period])%></Data></Cell>
				<% unless(params[:member_type]=='2') %>
				<Cell><Data ss:Type="String"><%=stat.count_auth %></Data></Cell>
				<% end %>
				<% unless(params[:member_type]=='1') %>			
				<Cell><Data ss:Type="String"><%=stat.count_no_auth %></Data></Cell>
				<% end %>
			</Row>
			<% end %>
			<% else %>
			<Row>
				<Cell><Data ss:Type="String"><%=t(:no_data)%></Data></Cell>
			</Row>
		<% end %>
    </Table>
  </Worksheet>
</Workbook>